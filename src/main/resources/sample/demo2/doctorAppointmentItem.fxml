<HBox alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="855.0" styleClass="btm-border" stylesheets="@css/style.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.demo2.DoctorAppointmentItemController">
